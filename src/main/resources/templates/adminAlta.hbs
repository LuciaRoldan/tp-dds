                    <a style="text-align:right;" href="/logout"> LOGOUT</a>
                </div>
                <div class="head-profile">
                    <img src="https://image.flaticon.com/icons/png/512/17/17797.png" alt="Profile photo">
                </div>
            </div>
        </div>
    </div>


    <div class="navigation">
        <div class="header-content">
            <div class="nav-top">
                <div class="nav-top-left">
                    <p>Administrador </p>
                </div>
                <div class="nav-top-right">
                </div>
                <div class="nav-bottom">
                </div>
            </div>
        </div>
    </div>

    <!-- Content -->
    <div class="content row" style="display:inline-block">
        <div class="averages" style="display:inline-block">


        </div>

        <!-- Second table of completed tests section-->
        <div class="complete-tests-title2" style="display: inline-block">
            <p>SELECCION DE DISPOSITIVO:</p>
            <ul>
                <div class="linkDispositivos">
                    <li><a href="/admin/altaDispositivo/aireAcondicionado"> Aire Acondicionado</a></li>
