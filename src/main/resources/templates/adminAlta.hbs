                    <li><a href="/reportes"> Heladera</a></li>
                    <li><a href="/usuario"> Lavarropas</a></li>
                    <li><a href="/dispositivo"> Ventilador</a></li>
                    <li><a href="/reportes"> Lampara</a></li>
                    <li><a href="/usuario"> PC</a></li>
                    <li><a href="/dispositivo"> Microondas</a></li>
                    <li><a href="/reportes"> Plancha</a></li>
                </div>

            </ul>
        </div>

        <div class="complete-tests-title2" style="display: inline-block">
            <ul>
                <div class="linkDispositivos">
                    {{#opciones}}
                    <li><a href={{link}}> {{opcion}}</a></li>
                    {{/opciones}}
                </div>
            </ul>
        </div>

        <form method="POST" action="/usuario" style="display: inline-block;height: 50%;margin-left:15px;">
            <br>
            <br>
            <div class="mdl-textfield mdl-js-textfield">
                <input class="mdl-textfield__input" type="text" id="ID" name="ID" />
                <label class="mdl-textfield__label" for="ID">ID CLIENTE </label>
            <button type="submit" style="display: inline-block;margin-left:100% " class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" value="CONSUMODISPOSITIVO ">ENVIAR</button>
            </div>
            </br>

            </form>


        </div>

        </div>

        <!-- Completed tests -->

</div>


